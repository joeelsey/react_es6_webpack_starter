{
  "name": "react_es6_webpack_starter",
  "version": "0.0.0",
  "description": "A starting point for an ES6 React Webpack project",
  "scripts": {
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base dist",
    "test": "npm run code-coverage",
    "test-react": "./node_modules/.bin/babel-node ./node_modules/.bin/mocha --require babel-core/register ./test --recursive --colors -R spec",
    "code-coverage": "./node_modules/.bin/babel-node ./node_modules/babel-istanbul/lib/cli.js cover --dir ./public/coverage ./node_modules/.bin/_mocha -- ./test --recursive",
    "build": "webpack --progress --colors --watch"
  },
  "author": "Rob Brennan <rob@therobbrennan.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-istanbul": "^0.7.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.2.0",
    "css-loader": "^0.23.1",
    "istanbul": "^0.4.2",
    "jsdom": "^6.5.1",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.7",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
  }
}
